{
  "deno.enablePaths": [
    "supabase/functions"
  ],
  "deno.lint": false,
  "deno.unstable": [
    "worker-options",
    "cron",
    "kv",
    "ffi",
    "fs",
    "http",
    "net"
  ],
  "deno.enable": false,
  "deno.path": "/usr/local/bin/deno",
  "[typescript]": {
    "editor.defaultFormatter": "denoland.vscode-deno"
  },
  "[markdown]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "prettier.tabWidth": 2,
  "prettier.useTabs": false,
  "markdownlint.config": {
    "MD022": false,
    "MD032": false,
    "MD036": false,
    "MD009": false
  }
}
