{
  "name": "dfs-manager-portal",
  "private": true,
  "version": "0.1.0",
  "type": "module",  "engines": {
    "node": ">=18.19.0 <19.0.0",
    "npm": ">=9.0.0"
  },
  "scripts": {
    "dev": "vite",
    "build": "tsc && vite build",
    "build:vercel": "node bulletproof-build.mjs",
    "build:vercel-simple": "node simple-build.mjs",
    "build:vercel-ultra": "node ultra-simple-build.mjs",
    "build:vercel-complex": "node vercel-build.mjs",
    "build:vercel-alt": "bash vercel-build.sh",
    "build:vercel-emergency": "node emergency-build.mjs",
    "build:dev": "tsc && vite build --mode development",
    "build:prod": "tsc && vite build --mode production --minify=true",
    "build:analyze": "vite build --mode production && npx vite-bundle-analyzer dist/assets/*.js",
    "lint": "eslint . --ext .ts,.tsx,.js,.jsx",
    "lint:fix": "eslint . --ext .ts,.tsx,.js,.jsx --fix",
    "lint:check": "eslint . --ext .ts,.tsx,.js,.jsx --max-warnings 0",
    "type-check": "tsc --noEmit",    "check-imports": "node scripts/check-imports.js",
    "verify-node": "node scripts/verify-node-version.mjs",
    "setup-hooks": "bash scripts/setup-git-hooks.sh",
    "dev:safe": "npm run lint:check && npm run type-check && npm run dev",
    "build:safe": "npm run lint:check && npm run type-check && npm run build",
    "build:production": "npm run quality-check && npm run build:prod",
    "quality-check": "npm run lint:check && npm run type-check && npm run check-imports",
    "preview": "vite preview",
    "serve": "vite preview --host",
    "test:production": "npm run build:production && npm run preview",
    "deploy:netlify": "npm run build:production && netlify deploy --prod --dir=dist",
    "deploy:vercel": "npm run build:production && vercel --prod",
    "docker:build": "docker build -t dfs-manager-portal .",
    "docker:run": "docker run -p 80:80 dfs-manager-portal",
    "docker:compose": "docker-compose up -d",
    "security:audit": "npm audit && npm run lint:check",
    "precommit": "npm run quality-check"
  },
  "dependencies": {
    "@anthropic-ai/sdk": "^0.53.0",
    "@aws-sdk/client-s3": "^3.826.0",
    "@aws-sdk/s3-request-presigner": "^3.826.0",
    "@azure/openai": "^2.0.0",
    "@hookform/resolvers": "^3.10.0",
    "@radix-ui/react-accordion": "^1.2.10",
    "@radix-ui/react-alert-dialog": "^1.1.13",
    "@radix-ui/react-aspect-ratio": "^1.1.6",
    "@radix-ui/react-avatar": "^1.1.9",
    "@radix-ui/react-checkbox": "^1.3.1",
    "@radix-ui/react-collapsible": "^1.1.10",
    "@radix-ui/react-context-menu": "^2.2.14",
    "@radix-ui/react-dialog": "^1.1.13",
    "@radix-ui/react-dropdown-menu": "^2.1.14",
    "@radix-ui/react-hover-card": "^1.1.13",
    "@radix-ui/react-label": "^2.1.6",
    "@radix-ui/react-menubar": "^1.1.14",
    "@radix-ui/react-navigation-menu": "^1.2.12",
    "@radix-ui/react-popover": "^1.1.13",
    "@radix-ui/react-progress": "^1.1.6",
    "@radix-ui/react-radio-group": "^1.3.6",
    "@radix-ui/react-scroll-area": "^1.2.8",
    "@radix-ui/react-select": "^2.2.4",
    "@radix-ui/react-separator": "^1.1.6",
    "@radix-ui/react-slider": "^1.3.4",
    "@radix-ui/react-slot": "^1.2.2",
    "@radix-ui/react-switch": "^1.2.4",
    "@radix-ui/react-tabs": "^1.1.11",
    "@radix-ui/react-toast": "^1.2.13",
    "@radix-ui/react-toggle": "^1.1.8",
    "@radix-ui/react-toggle-group": "^1.1.9",
    "@radix-ui/react-tooltip": "^1.2.6",
    "@supabase/supabase-js": "^2.39.3",
    "@tanstack/react-query": "^5.56.2",
    "@vitejs/plugin-react-swc": "^3.5.0",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "cmdk": "^1.1.1",
    "date-fns": "^3.6.0",
    "dotenv": "^16.5.0",
    "embla-carousel-react": "^8.6.0",
    "input-otp": "^1.4.2",
    "lucide-react": "^0.507.0",
    "motion": "^12.9.7",
    "next-themes": "^0.3.0",
    "openai": "^5.1.1",
    "react": "^18.3.1",
    "react-day-picker": "^8.10.1",
    "react-dom": "^18.3.1",
